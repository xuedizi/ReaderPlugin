<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.reader.qidian">

  <application
      android:label="笔趣爬虫MOD">
    <meta-data
        android:name="Default"
        android:value="true"/>
    <meta-data
        android:name="type"
        android:value="reader-plugin"/>
    <meta-data
        android:name="source"
        android:value="biqu"/>
    <meta-data
        android:name="priority"
        android:value="4"/>
    <meta-data
        android:name="class"
        android:value="com.biqu.Bq"/>

    <activity android:name="com.biqu.TestActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"></action>
        <category android:name="android.intent.category.LAUNCHER"></category>
      </intent-filter>
    </activity>
  </application>
</manifest>

