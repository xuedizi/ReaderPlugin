<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.reader.biantu">

  <application android:label="彼岸图爬虫MOD">

    <meta-data
        android:name="Default"
        android:value="true"/>
    <meta-data
        android:name="type"
        android:value="picture-plugin"/>
    <meta-data
        android:name="source"
        android:value="biantu"/>
    <meta-data
        android:name="priority"
        android:value="4"/>
    <meta-data
        android:name="class"
        android:value="com.biantu.Bat"/>

    <activity android:name="com.biantu.TestActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
  </application>

</manifest>