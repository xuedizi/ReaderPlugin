<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.reader.qinxiaoshuo">

  <application
      android:label="轻小说爬虫MOD">
    <meta-data
        android:name="type"
        android:value="reader-plugin"/>
    <meta-data
        android:name="source"
        android:value="qinxiaoshuo"/>
    <meta-data
        android:name="priority"
        android:value="8"/>
    <meta-data
        android:name="class"
        android:value="com.qinxiaoshuo.QinXs"/>

    <activity android:name="com.qinxiaoshuo.TestActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"></action>
        <category android:name="android.intent.category.LAUNCHER"></category>
      </intent-filter>
    </activity>
  </application>
</manifest>

