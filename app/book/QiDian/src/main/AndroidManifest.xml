<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.reader.qidian">

  <application
      android:label="起点爬虫MOD">
    <meta-data
        android:name="type"
        android:value="reader-plugin"/>
    <meta-data
        android:name="source"
        android:value="qidian"/>
    <meta-data
        android:name="priority"
        android:value="1"/>
    <meta-data
        android:name="class"
        android:value="com.qidian.Qd"/>

    <activity android:name="com.qidian.TestActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"></action>
        <category android:name="android.intent.category.LAUNCHER"></category>
      </intent-filter>
    </activity>
  </application>
</manifest>

