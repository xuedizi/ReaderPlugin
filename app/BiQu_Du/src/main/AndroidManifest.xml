<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.reader.biqudu">

  <application
      android:label="笔趣爬虫MOD(读)">
    <meta-data
        android:name="type"
        android:value="reader-plugin"/>
    <meta-data
        android:name="source"
        android:value="biqu_du"/>

    <meta-data
        android:name="priority"
        android:value="6"/>
    <meta-data
        android:name="class"
        android:value="com.biqu.du.Bq_Du"/>

    <activity android:name="com.biqu.du.TestActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"></action>
        <category android:name="android.intent.category.LAUNCHER"></category>
      </intent-filter>
    </activity>
  </application>
</manifest>

